      "confirmed": {
        "min": 0.70,
        "label": "Confirmed Execution"
      },
      "probable": {
        "min": 0.40,
        "label": "Probable Execution"
      },
      "weak": {
        "min": 0.20,
        "label": "Weak / Partial"
      },
      "not_proven": {
        "min": 0.0,
        "label": "Not Proven"
      }
    }
  },
  
  "semantic_rules": [
    {
      "rule_id": "exec_prefetch_run",
      "name": "Program Execution via Prefetch",
      "semantic_value": "Program Executed",
      "description": "Prefetch file indicates program was executed",
      "conditions": [
        {
          "feather_id": "prefetch",
          "field_name": "executable_name",
          "operator": "wildcard",
          "value": "*"
        }
      ],
      "logic_operator": "AND",
      "scope": "wing",
      "category": "process_execution",
      "severity": "info"
    },
    {
      "rule_id": "exec_shimcache_evidence",
